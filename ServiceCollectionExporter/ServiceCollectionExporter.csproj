<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>atawlee</Authors>
    <PackageId>ServiceCollectionExporter</PackageId>
    <Version>1.0.0</Version>
    <Description>Exports registered services from IServiceCollection to a JSON file.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/atawlee/ServiceCollectionExporter</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>DependencyInjection JSON Export</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="[6.0.0,)" />
    <PackageReference Include="System.Text.Json" Version="[6.0.10,)" />
  </ItemGroup>
</Project>
